<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="eCommerce.MAUI.Views.ConfigurationView"
             Title="ConfigurationView">
    <VerticalStackLayout>
        <Label Text="Tax" />
        <Entry Text="{Binding Tax}"/>
        <Button
            Text="Go Back"
            Clicked="BackClicked" />
    </VerticalStackLayout>
</ContentPage>